{
  "name": "@telega/utils",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./validation": {
      "import": "./dist/validation.js",
      "types": "./dist/validation.d.ts"
    },
    "./formatting": {
      "import": "./dist/formatting.js",
      "types": "./dist/formatting.d.ts"
    },
    "./parsers": {
      "import": "./dist/parsers.js",
      "types": "./dist/parsers.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "zod": "^3.22.0",
    "date-fns": "^2.30.0",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "tsup": "^8.0.0",
    "eslint": "^9.32.0",
    "prettier": "^3.6.2",
    "vitest": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4"
  }
}
